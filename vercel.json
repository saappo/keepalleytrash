{
  "version": 2,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/images/(.*)", "dest": "/public/images/$1" },
    { "src": "/css/(.*)", "dest": "/static/css/$1" },
    { "src": "/js/(.*)", "dest": "/static/js/$1" },
    { "src": "/(.*\\.png)", "dest": "/public/$1" },
    { "src": "/(.*\\.jpg)", "dest": "/public/$1" },
    { "src": "/(.*\\.jpeg)", "dest": "/public/$1" },
    { "src": "/(.*\\.gif)", "dest": "/public/$1" },
    { "src": "/(.*\\.svg)", "dest": "/public/$1" },
    { "src": "/(.*\\.ico)", "dest": "/public/$1" },
    { "src": "/(.*)", "dest": "/server.js" }
  ]
} 