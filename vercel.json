{
  "version": 2,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/images/(.*)", "dest": "/static/images/$1" },
    { "src": "/css/(.*)", "dest": "/static/css/$1" },
    { "src": "/js/(.*)", "dest": "/static/js/$1" },
    { "src": "/(.*)", "dest": "/server.js" }
  ]
} 